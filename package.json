  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/react-rendered-size.git"
  },
  "scripts": {
    "clean": "rimraf lib && rimraf dist",
    "dev": "NODE_ENV=development webpack-dev-server --progress --colors --config=webpack.config.dev.js",
