{
  "extends": ["airbnb"],
  "parser": "@typescript-eslint/parser",
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "implicit-arrow-linebreak": 0,
    "max-len": 0,
    "react/no-find-dom-node": 0
  }
}
